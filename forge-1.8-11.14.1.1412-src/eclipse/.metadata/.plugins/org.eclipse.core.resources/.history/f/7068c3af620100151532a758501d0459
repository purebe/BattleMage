package purebe.battlemage.magic;

import java.util.ArrayList;
import java.util.List;

import purebe.battlemage.magic.SpellCasting.SpellSymbol;

public final class Spells {
	public static class core {
		public static final List<SpellSymbol> Illuminate = new ArrayList<SpellSymbol>(3);
		
		public core() {
			Illuminate.add(SpellSymbol.White);
			Illuminate.add(SpellSymbol.Nature);
			Illuminate.add(SpellSymbol.Chaos);
		}
	}
}
