package purebe.battlemage.magic;

import net.minecraft.entity.player.EntityPlayer;

public class Illuminate implements ISpell {
	EntityPlayer player;
	int intensity;
	public Illuminate(EntityPlayer player) {
		this.player = player;
		this.intensity = 1;
	}

	@Override
	public void cast() {
		System.out.println("You cast Illuminate!");
		
	}
}
