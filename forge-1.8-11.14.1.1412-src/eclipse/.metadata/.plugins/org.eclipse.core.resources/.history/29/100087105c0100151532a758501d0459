package purebe.battlemage.magic;

import java.util.ArrayList;
import java.util.List;

import purebe.battlemage.magic.SpellCasting.SpellSymbols;

public final class Spells {
	public static List<SpellSymbols> Illuminate = new ArrayList(3);
	
	public Spells() {
		Illuminate.add(SpellSymbols.White);
		Illuminate.add(SpellSymbols.Nature);
		Illuminate.add(SpellSymbolls.Chaos);
	}
}
